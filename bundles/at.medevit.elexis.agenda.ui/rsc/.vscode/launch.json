{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Edge",
            "type": "edge",
            "request": "launch",
            "url": "${workspaceRoot}/html/debugParallel.html",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "trace": true,
            "userDataDir": "${workspaceRoot}/.vscode/edge"
        }
        {
            "name": "Launch localhost",
            "type": "edge",
            "request": "launch",
            "url": "http://localhost/html/debugParallel.html",
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach",
            "type": "edge",
            "request": "attach"
        },
        {
            "name": "Launch addon",
            "type": "edge",
            "request": "launch",
            "addonType": "addonSdk",
            "addonPath": "${workspaceRoot}"
        }
    ]
}