<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="at.medevit.elexis.impfplan.ui.VaccinationView"
            icon="icon://IMG_SYRINGE/icon.png"
            id="at.medevit.elexis.impfplan.ui.ImpfplanViewPart"
            inject="true"
            name="%vaccPlanViewPartName"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="at.medevit.elexis.impfplan.ui.view.vaccinationHeader"
            name="select vaccination header">
         <commandParameter
               id="at.medevit.elexis.impfplan.ui.view.vaccinationHeader.headerType"
               name="headerType"
               optional="true">
         </commandParameter>
      </command>
      <command
            id="at.medevit.elexis.impfplan.ui.command.ApplyVaccination"
            name="%applyVaccination">
      </command>
      <command
            id="at.medevit.elexis.impfplan.ui.command.SupplementVaccination"
            name="%supplementVaccination">
      </command>
      <command
            id="at.medevit.elexis.impfplan.ui.command.PrintVaccinationEntries"
            name="%printvaccinationentries">
      </command>
      <command
            id="at.medevit.elexis.impfplan.ui.command.SortByVaccination"
            name="%sortByVaccination">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:false"
               id="org.eclipse.ui.commands.toggleState">
         </state>
      </command>
      <command
            id="at.medevit.elexis.impfplan.ui.command.ImportOtherVaccinations"
            name="Andere Impfungen Importieren">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:at.medevit.elexis.impfplan.ui.ImpfplanViewPart">
       <command
               commandId="at.medevit.elexis.impfplan.ui.view.vaccinationHeader"
               icon="icon://IMG_FILTER/icon.png"
               id="filterMenuContribution"
               style="pulldown"
               tooltip="Darstellung der Impfungen definieren">
            <parameter
                  name="at.medevit.elexis.impfplan.ui.view.vaccinationHeader.headerType"
                  value="HWAV">
            </parameter>
         </command>
         <command
               commandId="at.medevit.elexis.impfplan.ui.command.ApplyVaccination"
               icon="icon://IMG_NEW"
               style="push"
               tooltip="%applyVaccination">
         </command>
         <command
               commandId="at.medevit.elexis.impfplan.ui.command.SupplementVaccination"
               icon="rsc/icons/inj_supplement.png"
               style="push"
               tooltip="%supplementVaccination">
         </command>
         <command
               commandId="at.medevit.elexis.impfplan.ui.command.PrintVaccinationEntries"
               icon="icon://IMG_PRINTER"
               style="push"
               tooltip="%printvaccinationentries">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:at.medevit.elexis.impfplan.ui.ImpfplanViewPart">
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:filterMenuContribution">
         <command
               commandId="at.medevit.elexis.impfplan.ui.command.SortByVaccination"
               icon="rsc/icons/sort-alphabet.png"
               style="toggle">
         </command>
         <separator
               name="at.medevit.elexis.impfplan.ui.separator1"
               visible="true">
         </separator>
         <dynamic
               class="at.medevit.elexis.impfplan.ui.handlers.FilterHandlerDynamicContribution"
               id="at.medevit.elexis.impfplan.ui.handlers.FilterHandlerDynamicContribution">
         </dynamic>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.VaccinationHeaderHandler"
            commandId="at.medevit.elexis.impfplan.ui.view.vaccinationHeader">
      </handler>
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.ApplyVaccinationHandler"
            commandId="at.medevit.elexis.impfplan.ui.command.ApplyVaccination">
      </handler>
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.PrintVaccinationEntriesHandler"
            commandId="at.medevit.elexis.impfplan.ui.command.PrintVaccinationEntries">
      </handler>
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.SupplementVaccinationHandler"
            commandId="at.medevit.elexis.impfplan.ui.command.SupplementVaccination">
      </handler>
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.SortByVaccinationHandler"
            commandId="at.medevit.elexis.impfplan.ui.command.SortByVaccination">
      </handler>
      <handler
            class="at.medevit.elexis.impfplan.ui.handlers.ImportLegacyVaccinationsHandler"
            commandId="at.medevit.elexis.impfplan.ui.command.ImportOtherVaccinations">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="ch.elexis.preferences.cdss"
            class="at.medevit.elexis.impfplan.ui.preferences.PreferencePage"
            id="at.medevit.elexis.impfplan.ui.preferences"
            name="%vaccPlanViewPartName">
      </page>
   </extension>

</plugin>
