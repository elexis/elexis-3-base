<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
         point="org.eclipse.ui.views">
      <view
            category="ch.elexis.omnivore"
            class="ch.elexis.omnivore.ui.views.OmnivoreView"
            icon="icons/fressen.gif"
            id="ch.elexis.omnivore.views.OmnivoreView"
            inject="true"
            name="Omnivore">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="ch.elexis.preferences.transporters"
            class="ch.elexis.omnivore.ui.preferences.PreferencePage"
            id="ch.elexis.omnivore.Preferences"
            name="Omnivore">
      </page>
   </extension>
<extension
         point="ch.elexis.core.ui.Transporter">
      <Transporter
            AcceptableTypes="ch.elexis.omnivore.data.DocHandle"
            id="ch.elexis.omnivore-plus"
            name="Omnivore plus"
            type="*"
            xChangeContribution="ch.elexis.omnivore.data.xChangeContributor">
      </Transporter>
   </extension>
   <extension
         point="ch.elexis.core.data.ServiceRegistry">
      <Service
            actor="ch.elexis.omnivore.data.DocumentManagement"
            description="Omnivore plus"
            name="DocumentManagement"
            value="90">
      </Service>
   </extension>
   <extension
         point="ch.elexis.core.ui.KonsExtension">
      <Link
            KonsExtension="ch.elexis.omnivore.ui.views.KonsExtension"
            name="omnivore">
      </Link>
   </extension>
</plugin>
